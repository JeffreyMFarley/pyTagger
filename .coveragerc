[run]
branch = True
source = pyTagger
omit =
    */__init__.py
    */__main__.py
    tests/*
    */snapshot_to_elasticsearch.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
ignore_errors = True