[run]
branch = True
source = pyTagger
omit =
    */__init__.py
    */__main__.py
    tests/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    except ImportError:
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    def buildArgParser()
ignore_errors = True
