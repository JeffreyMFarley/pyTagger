{
  "settings": {
    "analysis": {
      "char_filter": {
        "mapping_char_filter": {
          "type": "mapping",
          "mappings": [
            "&=> and ",
            "+=> and "
          ]
        }
      },
      "analyzer": {
        "normalized": {
          "type": "custom",
          "char_filter": ["mapping_char_filter"],
          "tokenizer": "whitespace",
          "filter": [
            "word_delimiter",
            "lowercase",
            "asciifolding",
            "stop",
            "unique"
          ]
        }
      }
    }
  },
  "mappings": {
    "track": {
      "_all": {
        "enabled": false
      },
      "properties": {
        "album": {
          "type": "string",
          "copy_to": "basic",
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "albumArtist": {
          "type": "string",
          "copy_to": "basic",
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "artist": {
          "type": "string",
          "copy_to": "basic",
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "basic": {
          "type": "string",
          "analyzer": "normalized"
        },
        "bitRate": {
          "type": "string"
        },
        "bpm": {
          "type": "string"
        },
        "comments": {
          "dynamic": true,
          "properties": {}
        },
        "compilation": {
          "type": "string"
        },
        "composer": {
          "type": "string"
        },
        "conductor": {
          "type": "string"
        },
        "disc": {
          "type": "string"
        },
        "encodingDate": {
          "type": "date",
          "ignore_malformed": true
        },
        "fileHash": {
          "type": "string",
          "index": "not_analyzed"
        },
        "genre": {
          "type": "string",
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "group": {
          "type": "string"
        },
        "id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "key": {
          "type": "string"
        },
        "language": {
          "type": "string"
        },
        "length": {
          "type": "integer",
          "ignore_malformed": true
        },
        "lyrics": {
          "dynamic": true,
          "properties": {}
        },
        "media": {
          "type": "string"
        },
        "originalReleaseDate": {
          "type": "date",
          "ignore_malformed": true
        },
        "path": {
          "type": "string",
          "index": "not_analyzed"
        },
        "playCount": {
          "type": "integer",
          "ignore_malformed": true
        },
        "publisher": {
          "type": "string",
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "recordingDate": {
          "type": "date",
          "ignore_malformed": true
        },
        "releaseDate": {
          "type": "date",
          "ignore_malformed": true
        },
        "remixer": {
          "type": "string",
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "subtitle": {
          "type": "string",
          "fields": {
            "djtagger": { 
              "type":  "date",
              "ignore_malformed": true
            }
          }
        },
        "taggingDate": {
          "type": "date",
          "ignore_malformed": true
        },
        "title": {
          "type": "string",
          "copy_to": "basic",
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "totalDisc": {
          "type": "integer",
          "ignore_malformed": true
        },
        "totalTrack": {
          "type": "integer",
          "ignore_malformed": true
        },
        "track": {
          "type": "integer",
          "ignore_malformed": true
        },
        "ufid": {
          "dynamic": true,
          "properties": {}
        },
        "vbr": {
          "type": "string"
        },
        "version": {
          "type": "string"
        },
        "year": {
          "type": "date",
          "ignore_malformed": true
        }
      }
    }
  }
}